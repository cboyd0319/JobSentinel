{
    "powerShellQA": {
        "defaultMode": "analyze",
        "autoFix": false,
        "severity": "Error", 
        "backupEnabled": true,
        "outputFormat": "Console",
        "excludePatterns": [
            "*backup*",
            "*temp*", 
            "*.bak",
            "*test*"
        ],
        "qualityThresholds": {
            "excellent": 95,
            "good": 80,
            "fair": 60,
            "poor": 40
        },
        "gitHookMode": true,
        "continuousMonitoring": false
    }
}
